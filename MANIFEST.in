# https://packaging.python.org/guides/using-manifest-in/

include LICENSE
include CODEOWNERS
include MANIFEST.in

include *.py
include *.rst
include *.md
include *.txt
include *.cpp
graft returnn

# Not all extern files.
prune returnn/extern_private
prune returnn/extern
